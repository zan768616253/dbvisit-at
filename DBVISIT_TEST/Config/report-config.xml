<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
	<configuration>
		<!-- report theme -->
		<!-- standard, dark -->
		<theme>dark</theme>

		<!-- enables timeline -->
		<!-- defaults to true -->
		<enableTimeline>true</enableTimeline>

		<!-- document encoding -->
		<!-- defaults to UTF-8 -->
		<encoding>UTF-8</encoding>

		<!-- protocol for script and stylesheets -->
		<!-- defaults to https -->
		<protocol>https</protocol>

		<!-- title of the document -->
		<documentTitle>DBVisit Specflow Report</documentTitle>

		<!-- report name - displayed at top-nav -->
		<reportName>Report Demonstration with NUnit and Extent Reports</reportName>

		<!-- report headline - displayed at top-nav, after reportHeadline -->
		<reportHeadline>Report Demonstration with NUnit and Extent Reports</reportHeadline>

		<!-- global date format override -->
		<!-- defaults to yyyy-MM-dd -->
		<dateFormat>MM-dd-yyyy</dateFormat>

		<!-- chart visibility toggle -->
		<showChart>true</showChart>

		<!-- offline report -->
		<offlineMode>true</offlineMode>

		<!-- global time format override -->
		<!-- defaults to HH:mm:ss -->
		
		<timeFormat>hh:mm:ss tt</timeFormat>
		<chartVisibilityOnOpen>true</chartVisibilityOnOpen>
		<testViewChartLocation>top</testViewChartLocation>
		<takeScreenshotOnFail>true</takeScreenshotOnFail>
		<projectName>DBVISIT</projectName>
		<!-- custom javascript -->
		<scripts>
			<![CDATA[
 $(document).ready(function() {

  });
   ]]>
		</scripts>

		<!-- custom styles -->
		<styles>
			<![CDATA[

 ]]>
		</styles>
	</configuration>
</extentreports>